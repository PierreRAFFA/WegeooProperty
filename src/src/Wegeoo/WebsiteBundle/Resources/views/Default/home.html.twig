<!DOCTYPE html>
<html lang="en-US" xmlns="http://www.w3.org/1999/xhtml">
	<head>

        <!-- META -->
        <meta http-equiv="Content-Type" content="text/html;charset=utf-8" />
        <meta property="og:title" content="{{title}}" />
        <title>{{title}}</title>
        <meta name="description" content="{{description}}"/>
        <meta name="keywords" content="flat, geolocation, classified, classifieds, estate, property, search, rent, sale, rentals, geolocated, map, united kingdom" />
        <meta name="viewport" content="width=device-width, initial-scale=1.0"/>
        <meta name="viewport" content="initial-scale=1.0, user-scalable=no" />

        <!-- Google Webmaster Tools -->
        <meta name="google-site-verification" content="RceqAnyw81SL8OGr4SdWofijewRHEqh2ij-3jmmNlUI" />

		<!--FAVICON-->
		<link rel="icon" type="image/gif" href="/favicon.ico" />
		
		<!--JQUERY-->
    	<script src="{{ asset('js/jquery.js') }}"></script>
    	
		<!--JQUERY UI-->
    	<script src="{{ asset('bundles/wegeoowebsite/js/jquery-ui-1.11.3/jquery-ui.min.js') }}"></script>

    	<!--ANGULAR JS -->
    	{#<script src="https://ajax.googleapis.com/ajax/libs/angularjs/1.3.5/angular.min.js"></script>#}
    	
		<!-- IntÃ©gration de Bootstrap -->
		<link rel="stylesheet" media="screen" href="{{ asset('css/bootstrap.css') }}">
		<script src="{{ asset('js/bootstrap.js') }}"></script>
		
		<!-- GOOGLE MAPS -->
		<script type="text/javascript" src="http://maps.googleapis.com/maps/api/js?sensor=false"></script>
		
		<!-- WEGEOO ENGINE LIB -->
		{% block stylesheets %}
			{% stylesheets output="css/compiled/wegeoo-engine1.css" filter="less"
            'bundles/wegeoowebsite/wegeoo/component/alert/bootstrap-dialog.css'
            'bundles/wegeoowebsite/wegeoo/component/multiselect/bootstrap-select.css'
            'bundles/wegeoowebsite/wegeoo/component/autocomplete/jquery.autocomplete.css'
            'bundles/wegeoowebsite/css/header.less'
            'bundles/wegeoowebsite/css/home.css'
            'bundles/wegeoowebsite/css/home-992.css'
            'bundles/wegeoowebsite/css/home-768.less'
            'bundles/wegeoowebsite/css/home-512.css'
            'bundles/wegeoowebsite/css/home-380.css'
            'bundles/wegeoowebsite/css/searchFormView.css'
            'bundles/wegeoowebsite/css/footer.css'
            'bundles/wegeoowebsite/css/sideMenu.less'

            %}
            	<link type="text/css" media="screen" rel="stylesheet" href="{{ asset_url }}" />
			{% endstylesheets %}
		{% endblock %}
		
		{% block javascripts %}
			{% javascripts output="js/compiled/wegeoo-engine.js"
            'bundles/wegeoowebsite/wegeoo/Wegeoo.js'
            'bundles/wegeoowebsite/wegeoo/component/alert/bootstrap-dialog.js'
            'bundles/wegeoowebsite/wegeoo/component/banner/wegeoo.banner.js'
            'bundles/wegeoowebsite/wegeoo/component/energeticArrow/wegeoo.energeticarrow.js'
            'bundles/wegeoowebsite/wegeoo/component/sideMenu/ui.js'
            'bundles/wegeoowebsite/wegeoo/component/multiselect/bootstrap-select.js'
            'bundles/wegeoowebsite/wegeoo/component/tabs/bootstrap-tabs.js'
            'bundles/wegeoowebsite/wegeoo/component/infinitescroll/infiniteScroll.js'
            'bundles/wegeoowebsite/wegeoo/component/wegeoomap/WegeooMap.js'
            'bundles/wegeoowebsite/wegeoo/component/wegeoomap/markerclusterer.js'
            'bundles/wegeoowebsite/wegeoo/component/autocomplete/jquery.autocomplete.js'
            'bundles/wegeoowebsite/wegeoo/component/swiper/idangerous.swiper.min.js'
            'bundles/wegeoowebsite/wegeoo/controller/FrontController.js'
            'bundles/wegeoowebsite/wegeoo/controller/StorageController.js'
            'bundles/wegeoowebsite/wegeoo/controller/ClassifiedController.js'
            'bundles/wegeoowebsite/wegeoo/controller/CityController.js'
            'bundles/wegeoowebsite/wegeoo/route/RouteController.js'
            'bundles/wegeoowebsite/wegeoo/route/Route.js'
            'bundles/wegeoowebsite/wegeoo/route/Slug.js'
            'bundles/wegeoowebsite/wegeoo/route/SlugView.js'
            'bundles/wegeoowebsite/wegeoo/locale/LocaleManager.js'
            'bundles/wegeoowebsite/wegeoo/event/WegeooEvent.js'
            'bundles/wegeoowebsite/wegeoo/service/Service.js'
            'bundles/wegeoowebsite/wegeoo/service/Operation.js'
            'bundles/wegeoowebsite/wegeoo/utils/Ini.js'
            'bundles/wegeoowebsite/wegeoo/utils/StringUtils.js'
			%}
				<script src="{{ asset_url }}" charset="UTF-8"></script>
			{% endjavascripts %}
		{% endblock %}

        <!-- PAGE BEHAVIOR -->
        <script src="{{ asset('bundles/wegeoowebsite/js/homePageView.js') }}" charset="UTF-8"></script>
        <script src="{{ asset('bundles/wegeoowebsite/wegeoo/controller/HomePageViewController.js') }}" charset="UTF-8"></script>

        <!-- GOOGLE ANALYTICS -->
        <script>
            (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
                (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
                    m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
            })(window,document,'script','//www.google-analytics.com/analytics.js','ga');

            ga('create', 'UA-59398211-1', 'auto');
            ga('send', 'pageview');

        </script>

		<!-- BOOTSTRAP SLIDER -->
		<!-- <script src="/libs/bootstrap/js/bootstrap-slider.js"></script>
		<script src="/libs/bootstrap/js/modernizr.js"></script>
		<link href="/libs/bootstrap/css/bootstrap-slider.css" rel="stylesheet"> -->
		
		<!--FONTS-->
		<!--<link rel='stylesheet' type='text/css' href='http://fonts.googleapis.com/css?family=Maven+Pro:400,500,700' >-->
		
		<script>
			//var app = angular.module("WegeooApp" , []);

			var gCityName 		= "{{cityName}}";
			var gCityPostCode   = "{{cityPostCode}}";
            var gBaseURL        = "{{baseURL}}";
            var gAssetDirectory = "{{ asset('bundles/wegeoowebsite/') }}";

			//@TODO locale symfony
			var lStrings = {};
            lStrings.sale = "{% trans %} wegeoo.sale {% endtrans %}";
            lStrings.rent = "{% trans %} wegeoo.rent {% endtrans %}";



		</script>
		<!--[if gte IE 9]>
		  <style type="text/css">
		    .gradient {
		       filter: none;
		    }
		  </style>
		<![endif]-->
	</head>
	
	<body>

        <input style="display:none;" id="theme" value="{{ wegeoo_type }}"/>

		<div id="mainContainer" class="container container-wegeoo">

            {% include "WegeooWebsiteBundle:Default:sideMenu.html.twig" %}

            <!-- HEADER -->
            <header class="row show-grid logoLayout">
                <div class="span12">

                    <!-- LOGO -->
                    <div class="logo">
                        <img src="{{ asset("bundles/wegeoowebsite/images/logo180px.png") }}" width="90" height="90" alt="Wegeoo Logo"/>
                    </div>

                    <!-- TEXT -->
                    <div class="logo-text">
                        <h1>
                            <span class="logoName1">we</span><span class="logoName2">Geoo</span>
                            <br/>
                            <span class="logo-text-span">{% trans %} wegeoo.slogan.part1 {% endtrans %}</span>
                        </h1>
                    </div>
                </div>
            </header>

            <!-- AD -->
            <div class="row show-grid row-margin">
                <div class="col-xs-12 col-sm-12 col-md-12 ad">
                    <img src="{{ asset('bundles/wegeoowebsite/images/temp/adHome2.png') }}" alt="ad"/>
                </div>
            </div>

            <!-- SEARCH -->
            <div class="row show-grid searchCategoryRow">

                <!-- CATEGORY SELECTION -->
                <div class="searchCategoryContainer">
                    <ul id="searchCategory" class="nav nav-tabs searchCategory" data-wegeoo-slug="categoryLocaleName" data-wegeoo-component="tabs">
                        <li class="searchCategoryLi">
                            <a href="#"  		data-value="sale" data-toggle="tab" rel="nofollow">{% trans %} classified.category.sale {% endtrans %}</a></li>
                        <li class="searchCategoryLi active">
                            <a href="#" 		data-value="rent" data-toggle="tab" rel="nofollow">{% trans %} classified.category.rent {% endtrans %}</a></li>
                        {#<li class="searchCategoryLi disabled disabledTab">#}
                        {#<a href="#season"  		data-toggle="tab">{% trans %} classified.category.holidayRentals {% endtrans %}</a></li>#}
                        {#<li class="searchCategoryLi disabled disabledTab">#}
                        {#<a href="#new"  		data-toggle="tab">{% trans %} classified.category.new {% endtrans %}</a></li>#}
                        {#<li class="searchCategoryLi disabled disabledTab">#}
                        {#<a href="#couchsurfing" data-toggle="tab">{% trans %} classified.category.couchSurfing {% endtrans %}</a></li>#}
                    </ul>

                    <!-- CATEGORY CONTENTS -->
                    <div class="tab-content">

                        <!-- SALE -->
                        <div class="tab-pane"></div>

                        <!-- SHARE -->
                        <div class="tab-pane active"></div>

                        <!-- SEASON -->
                        <div class="tab-pane"></div>

                        <!-- NEW -->
                        <div class="tab-pane"></div>

                        <!-- COUCH SURFING -->
                        <div class="tab-pane"></div>
                    </div>
                </div>
            </div>

            <!-- SUBMIT BUTTON -->
            <a href="{{baseURL}}/{% trans %}wegeoo.property{% endtrans %}/{% trans %}wegeoo.post{% endtrans %}">
                <button class="btn btn-primary btn-large postButton" type="button">
                    <img class="searchIcon" src="{{ asset('bundles/wegeoowebsite/images/pen.png') }}" width="23" height="23"/>
                    <div class="postText">{% trans %}wegeoo.post.title {% endtrans %}</div>
                </button>
            </a>

            <!-- SEARCH -->
            <div id="mapLayout" class="row show-grid row-margin row-colored row-search">


                {% include "WegeooWebsiteBundle:Default:homeSearchForm.html.twig" %}


                <div class="mapContainer">
                    <div id="wegeooMap" class="map_canvas" style="width:100%; height:300px"
                         data-wegeoo-slug="map" data-wegeoo-component="wegeooMap" mapLatitude="51.507351" mapLongitude="-0.127758"></div>

                    <!-- SUBMIT BUTTON -->
                    {#<div class="displayButtons">#}
                        {#<button id="displayMarkerAnnouncementsButtonSubmit" class="btn btn-primary btn-large displayButtonSubmit" type="submit">{% trans %}search.displayClassifiedsFromSelectedMarkers {% endtrans %}</button>#}
                        {#<a id="mapToolTip"#}
                           {#class="mapToolTip"#}
                           {#data-toggle="tooltip"#}
                           {#data-original-title="{% trans %}search.displayClassifiedsFromSelectedMarkers.toolTip {% endtrans %}">#}
                            {#<button id="helpButton" class="btn btn-primary btn-large helpButton" type="submit">?</button>#}
                        {#</a>#}
                    {#</div>#}


                    <div id="mapLoading" class="mapLoading">
                        <div class="map_waiting" >
                            <div class="map_waiting_text"><span>{% trans %}search.map.loading {% endtrans %}</span></div>
                            <div class="map_waiting_icon"></div>
                        </div>
                    </div>
                </div>


                <!-- MAP REPLACEMENT -->
                <div id="mapReplacementLayout" class="row show-grid mapReplacementLayout">
                    <div class="col-xs-12 col-sm-12 col-md-12 nopadding"></div>
                </div>

                <!-- SCROLL ICON only for mobile -->
                <div class="scrollIcon">
                    <img src="/bundles/wegeoowebsite/images/scrollIcon.png" width="15" height="30">
                </div>

           </div>

           <!-- ANNOUNCEMENT HEADERS -->
            <div class="row show-grid row-margin row-colored bannerLayout">
                <div class="col-xs-12 col-sm-12 col-md-12 nopadding">

                    <div class="bannerTitle">
                        <span class="banner-text1">{% trans %}wegeoo.lastClassifiedsIn{% endtrans %}&nbsp;</span><span id="cityName" class="banner-text2">{{cityName}}</span>
                    </div>

                    <div id="banner" class="bannerContainer">

                    </div>
                </div>
            </div>

			<!-- RESULT -->
			<div class="row show-grid">

				<!-- SPECIALISTS -->
				<div class="col-xs-12 col-sm-12 col-md-12 specialistLayout nopadding">
					
					<!-- SPECIALIST TITLE -->
					<div class="specialistTitle">
						<span class="specialistTitleSpan1">L</span><span class="specialistTitleSpan2">OCAL </span>
						<span class="specialistTitleSpan1">A</span><span class="specialistTitleSpan2">GENCIES </span>
					</div>
					
					<!-- SPECIALIST LIST -->
					<div class="specialistBannerList">
						
					</div>
					
					<!-- SPECIALIST BANNER PUB -->
					<div class="specialistBannerPub">
						<span class="specialistBannerPubSpan1">Y</span><span class="specialistBannerPubSpan2">OU HAVE AN</span>
						<span class="specialistBannerPubSpan3">ESTATE AGENCY </span>
						<span class="specialistBannerPubSpan2">AND WANT TO</span>
						<span class="specialistBannerPubSpan3">APPEAR IN THIS LIST</span><br/>
						<span class="specialistBannerPubSpan4"><a href="mailto:wegeoo.uk@gmail.com">>> CONTACT US</a></span>
					</div>
					
				</div>
				
				<!-- RESULT -->
				<div class="resultLayoutContainer">
					
					<!-- SORT --> 
					<div class="resultLayoutSort">{% trans %}classifiedList.sortBy {% endtrans %} :
				      	<select id="resultLayoutSortList" class="selectpicker" 
						 data-style="resultLayoutSortSelect">
							<option value="priceASC" selected>{% trans %}classifiedList.sortBy.leastExpensive {% endtrans %}</option>
							<option value="priceDESC">{% trans %}classifiedList.sortBy.mostExpensive {% endtrans %}</option>
							<option value="modificationDateDESC">{% trans %}classifiedList.sortBy.mostRecent {% endtrans %}</option>
						</select>
					</div>
					
					<!-- RESULTS --> 
					<div id="resultLayout" class="col-xs-12 col-sm-12 col-md-12 resultLayout nopadding">
						
					</div>

				</div>
			</div>
			
            {% embed "@WegeooWebsite/Default/footer.html.twig" %}{% endembed %}

		</div>

	</body>
</html>